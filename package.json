{
  "name": "hrm-package",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "move:prod": "rmdir /s /q dist_prod && robocopy dist dist_prod /MOVE /E",
    "build:prod": "copy .env.prod .env.production && vite build && npm run move:prod",
    "move:test": "rmdir /s /q dist_test && robocopy dist dist_test /MOVE /E",
    "build:test": "copy .env.test .env.production && vite build && npm run move:test",
    "build:all": "tsc && npm run build:test && npm run build:prod",
    "create:test": "mkdir dist_test",
    "create:prod": "mkdir dist_prod",
    "create:all": "npm run create:test && npm run create:prod",
    "build:normal": "tsc && copy .env.test .env.production && vite build",
    "build:mac": "tsc && npm run build:test-mac && npm run build:prod-mac",
    "build:test-mac": "cp .env.test .env.production && vite build && cp -r dist/* dist_test",
    "build:prod-mac": "cp .env.prod .env.production && vite build && cp -r dist/* dist_prod"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@rsuite/icons": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@tinymce/tinymce-react": "^4.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.36",
    "admin-lte": "^3.2.0",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "formik": "^2.2.9",
    "fs-extra": "^10.1.0",
    "js-cookie": "^3.0.1",
    "leaflet": "^1.9.3",
    "luxon": "^2.4.0",
    "moment": "^2.29.3",
    "pdfjs": "^2.4.7",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-bootstrap": "^2.4.0",
    "react-confirm-alert": "^3.0.2",
    "react-dom": "^18.2.0",
    "react-google-charts": "^4.0.0",
    "react-icons": "^4.4.0",
    "react-leaflet": "^4.2.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-table": "^7.8.0",
    "react-toastify": "^9.0.1",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "rsuite": "^5.13.1",
    "styled-components": "^5.3.6",
    "tinymce": "^6.2.0",
    "ts-pattern": "^4.1.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
